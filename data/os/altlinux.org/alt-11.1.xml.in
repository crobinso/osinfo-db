  <os id="http://altlinux.org/alt/11.1">
    <short-id>alt11.1</short-id>
    <name>ALT 11.1</name>
    <version>11.1</version>
    <vendor>BaseALT</vendor>
    <family>linux</family>
    <distro>alt</distro>
    <codename>Prometheus</codename>
    <upgrades id="http://altlinux.org/alt/11.0"/>
    <derives-from id="http://altlinux.org/alt/11.0"/>

    <release-date>2025-09-03</release-date>
    <eol-date>2027-12-31</eol-date>

    <variant id="workstation">
      <name>ALT Workstation 11.1</name>
    </variant>
    <variant id="kworkstation">
      <name>ALT Workstation K 11.1</name>
    </variant>
    <variant id="kworkstation-live">
      <name>ALT Workstation K Live 11.1</name>
    </variant>

    <!-- Workstation -->
    <media arch="x86_64">
      <variant id="workstation"/>
      <url>http://ftp.altlinux.ru/pub/distributions/ALTLinux/p11/images/workstation/x86_64/alt-workstation-11.1-x86_64.iso</url>
      <iso>
        <system-id>LINUX</system-id>
        <volume-id>ALT Workstation 11\.1(\.\d+)* x86_64</volume-id>
        <publisher-id>BASEALT LTD</publisher-id>
        <application-id>ALT WORKSTATION 11\.1(\.\d+)* X86_64.*</application-id>
      </iso>
      <kernel>boot/vmlinuz</kernel>
      <initrd>boot/initrd.img</initrd>
    </media>

    <media arch="aarch64">
      <variant id="workstation"/>
      <url>http://ftp.altlinux.ru/pub/distributions/ALTLinux/p11/images/workstation/aarch64/alt-workstation-11.1-aarch64.iso</url>
      <iso>
        <system-id>LINUX</system-id>
        <volume-id>ALT Workstation 11\.1(\.\d+)* aarch64</volume-id>
        <publisher-id>BASEALT LTD</publisher-id>
        <application-id>ALT WORKSTATION 11\.1(\.\d+)* AARCH64.*</application-id>
      </iso>
      <kernel>boot/vmlinuz</kernel>
      <initrd>boot/initrd.img</initrd>
    </media>

    <!-- Workstation K -->
    <media arch="x86_64">
      <variant id="kworkstation"/>
      <url>http://ftp.altlinux.ru/pub/distributions/ALTLinux/p11/images/kworkstation/alt-kworkstation-11.1-install-x86_64.iso</url>
      <iso>
        <system-id>LINUX</system-id>
        <volume-id>ALT Workstation K 11\.1(\.\d+)*</volume-id>
        <publisher-id>BASEALT LTD</publisher-id>
        <application-id>ALT WORKSTATION K 11\.1(\.\d+)* X86_64.*</application-id>
      </iso>
      <kernel>boot/vmlinuz</kernel>
      <initrd>boot/initrd.img</initrd>
    </media>

    <!-- Workstation K Live-->
    <media arch="x86_64" live="true">
      <variant id="kworkstation-live"/>
      <url>http://ftp.altlinux.ru/pub/distributions/ALTLinux/p11/images/kworkstation/alt-kworkstation-11.1-live-x86_64.iso</url>
      <iso>
        <system-id>LINUX</system-id>
        <volume-id>ALT Workstation K 11\.1(\.\d+)* Live</volume-id>
        <publisher-id>BASEALT LTD</publisher-id>
        <application-id>ALT WORKSTATION K 11\.1(\.\d+)* LIVE X86_64.*</application-id>
      </iso>
      <kernel>boot/vmlinuz</kernel>
      <initrd>boot/initrd.img</initrd>
    </media>

    <resources arch="all">
      <minimum>
        <n-cpus>1</n-cpus>
        <cpu>1000000000</cpu>
        <ram>536870912</ram>
        <storage>10737418240</storage>
      </minimum>
      <recommended>
        <ram>2147483648</ram>
        <storage>21474836480</storage>
      </recommended>
    </resources>

  </os>
</libosinfo>
