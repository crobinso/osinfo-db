  <os id="http://libosinfo.org/linux/2024">
    <short-id>linux2024</short-id>
    <name>Generic Linux 2024</name>
    <version>2024</version>
    <vendor>Libosinfo</vendor>
    <family>linux</family>
    <derives-from id="http://ubuntu.com/ubuntu/24.04"/>

    <release-date>2024-01-01</release-date>
    <eol-date>2029-01-01</eol-date>

    <resources arch='all' inherit='true'/>

    <!-- ubuntu 24.04, which we inherit from, has virtio1.0-gpu
         explicitly disabled, due to an apparent bug on ubuntu side
         from 22.04 timeframe.

         However virtio-gpu has been working in similar distros for years.
         So we explicitly enable it for this generic entry. -->
    <devices>
      <device id="http://pcisig.com/pci/1af4/1050" supported="true"/> <!-- virtio1.0-gpu -->
    </devices>
  </os>
</libosinfo>
