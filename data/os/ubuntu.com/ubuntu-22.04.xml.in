    <image arch="x86_64" format="containerdisk" cloud-init="true">
      <url>docker://quay.io/containerdisks/ubuntu:22.04</url>
    </image>

    <installer>
      <script id='http://ubuntu.com/ubuntu/preseed/desktop'/>
